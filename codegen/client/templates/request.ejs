import { <%= serviceName %> } from '../<%= serviceName %>';
import { <%= serviceName %>Response } from '../types/base-response';

export type <%= requestName %> = <%= requestType %>;

export type <%= responseName %> = <%= responseType %>;

export async function <%= functionName %>(
	this: <%= serviceName %>,
	request: <%= requestName %>,
): Promise<<%= serviceName %>Response<<%= responseName %>>> {
	const data = <%= dataObject %>;
	const params = <%= paramsObject %>;
	return this.makeRequest<<%= responseName %>>({
		method: '<%= method %>',
		url: `<%= route %>`,
		data,
		params,
	});
}
